  "type": "module",
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@types/react-dom": "^18.0.6",
    "formik": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-type-animation": "^3.1.0",
    "react-uuid": "^2.0.0",
